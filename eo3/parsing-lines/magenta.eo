+package eo3.parsing-lines
+version 0.0.0

# Highlight with magenta color all unprintable characters
# in the string.
[txt] > magenta
  replaced. > @
    QQ.txt.text
      txt
    compiled.
      QQ.txt.regex
        string
          2F-00-2F
    QQ.txt.text
      "\033[95m?\033[0m"
